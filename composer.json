{
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "ext-gd": "*",
    "php": "~7.4.0",
    "wpackagist-plugin/akismet": "^4.1.0",
    "wpackagist-plugin/all-in-one-wp-migration": "^7.24",
    "wpackagist-plugin/amazon-s3-and-cloudfront": "2.4.1",
    "wpackagist-plugin/filebird": "^4.1",
    "wpackagist-plugin/insert-html-snippet": "^1.2.5",
    "wpackagist-plugin/official-mailerlite-sign-up-forms": "^1.4",
    "wpackagist-plugin/perfect-woocommerce-brands": "^1.7.0",
    "wpackagist-plugin/regenerate-thumbnails": "3.1.3",
    "wpackagist-plugin/resize-image-after-upload": "1.8.6",
    "wpackagist-plugin/smart-slider-3": "^3.4.1",
    "wpackagist-plugin/woocommerce": "7.1.0",
    "wpackagist-plugin/wp-user-avatar": "^2.2.5",
    "wpackagist-theme/storefront": "^2.3.5",
    "wpackagist-plugin/woocommerce-delivery-notes": "^4.6",
    "wpackagist-plugin/mailin": "^3.1",
    "wp-cli/wp-cli-bundle": "^2.5",
    "wpackagist-plugin/simple-banner": "^2.10",
    "wpackagist-plugin/woo-advanced-shipment-tracking": "3.2.4.1",
    "wpackagist-plugin/facebook-for-woocommerce": "^3.0",
    "wpackagist-plugin/yith-paypal-express-checkout-for-woocommerce": "^1.13",
    "wpackagist-plugin/waitlist-woocommerce": "^2.5"
  },
  "scripts": {
    "post-install-cmd": "wp plugin activate akismet woocommerce insert-html-snippet perfect-woocommerce-brands amazon-s3-and-cloudfront wp-user-avatar smart-slider-3 regenerate-thumbnails resize-image-after-upload facebook-for-woocommerce filebird official-mailerlite-sign-up-forms woocommerce-delivery-notes mailin simple-banner woo-advanced-shipment-tracking yith-paypal-express-checkout-for-woocommerce waitlist-woocommerce; wp theme activate storefront-child",
    "test": "phpunit -v tests"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5.0"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
